<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
        <title>{% if title %}{{ title }} &ndash; {% endif %}{{ site.title }}</title>
        {% if site.description %}
        <meta name="description" content="{{ site.description }}">
        {% endif %}
        
        <link rel="stylesheet" href="/css/style.css" media="screen">
        <link rel="canonical" href="{{ site.url }}{{ page.url }}">
        <link rel="shortcut icon" href="/images/nelf-priest-avatar.gif" type="image/gif" />
        <link rel="icon" href="/images/nelf-priest-avatar.gif" type="image/gif" />
        <link rel="apple-touch-icon" href="/images/nelf-priest-avatar.gif" />
        <meta name="msapplication-TileImage" content="/images/nelf-priest-avatar.gif" />

        <link rel="alternate" type="application/atom+xml" title="{{ site.title }} Â» Feed" href="{{ site.url }}/feed.xml">
        <script>
          var _paq = window._paq = window._paq || [];
          /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
          _paq.push(['trackPageView']);
          _paq.push(['enableLinkTracking']);
          (function() {
            var u="//a.faarao.net/";
            _paq.push(['setTrackerUrl', u+'matomo.php']);
            _paq.push(['setSiteId', '1']);
            var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
            g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
          })();
        </script>
    
    </head>
      

  <body>
    <div class="container">
      <header class="site-header">
          <a class="site-title" href="{{ site.url }}">{{ site.title }}</a>
          <span>{{ site.description }}</span>
      </header>
      <main>
        {{ content | safe }}
      </main>
    </div>

  </body>

</html>
